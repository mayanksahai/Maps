{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "amd": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "react/prop-types": 0,
        "global-require": "off"
    }
}